<div style={{ height: 400, width: '100%' }}>
  <DataGrid
    rows={rows}
    columns={columns}
    editRowsModel={editRowsModel}
    editMode="row"
    onEditRowsModelChange={handleEditRowsModelChange}
  />
</div>
<Alert severity="info" style={{ marginTop: 8 }}>
  <code>editRowsModel: {JSON.stringify(editRowsModel)}</code>
</Alert>